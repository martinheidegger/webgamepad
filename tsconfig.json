{
  "compilerOptions": {
    "strict": true,
    "strictBindCallApply": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "alwaysStrict": true,
    "rootDir": "src",
    "target": "ES2020",
    "lib": ["DOM", "ES2020"],
    "moduleResolution": "NodeNext",
    "module": "System",
    "jsx": "react-jsx",
    "outFile": "site/app.js"
  }
}